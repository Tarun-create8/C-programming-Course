#include <stdio.h>
#include <stdlib.h>
int main(){
    int number, iteration, result=1;
    printf("Enter number: ");
    scanf("%d", &number);  
    printf("Enter iteration: ");
    scanf("%d", &iteration);
    for(int i=1; i<=iteration; i++){
        result=number*i;
        printf("%d=%d*%d\n", result, number, i);
    }
    
    return 0;
}


//Fibonacci Series--->>
#include <stdio.h>
#include <stdlib.h>
int main(){
    int n, result=0;
    printf("Enter number of iterations: ");
    scanf("%d", &n);
    int t1=0, t2=1;
    printf("Sequence is : \n%d\n%d\n", t1, t2);
    for(int i=0; i<=n; i++){
        result = t1+t2;
        t1=t2;
        t2=result;
        printf("%d\n", result);
    }
    return 0;
}
