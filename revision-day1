#include <stdio.h>
#include <stdlib.h>
int main(){
    float num1, num2;
    printf("Mul: %.2f", num1*num2);
    return 0;
}

7th 
#include <stdio.h>
int main(){
    char myChar1;
    printf("Enter character: ");
    scanf("%c", &myChar1);
    prinf("To print character: %c", myChar1);
    prinf("To print character: %d", myChar1);
    prinf("To print character: %X", myChar1);// HExa decimeal
    return 0;    
}

//Factorial claculation 
#include <stdio.h>
int main() {
	int n, result=1;
	printf("Enter N: ");
	scanf(" %d", &n);
	if(n==0){
	    printf("0! = 1\n");
	}
	else if(n>0){
	    for(int i = 1; i<=n ; i++){
	        result = result * (i);
	    }
	    printf("%d\n", result);
	}
	else {
	    printf("Enter a positive number\n");
	}
    return 0;
}















