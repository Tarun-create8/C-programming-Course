    return 0;
}

//4th 
#include <stdio.h>
#include <stdlib.h>
int main(){
    int a, b;
    printf("Enter value of a & b: ");
    scanf("%d\n%d", &a, &b);
    printf("Adiition: %d", a+b);
    return 0;
}

5
#include <stdio.h>
#include <stdlib.h>
int main(){
    int num;
    printf("Enter the number: ");
    scanf("%d", &num);
    for(int i=1; i<=num; i++){
        if(num%1==0 || num%num==0){
        printf("Number is prime\n");
        }
        else{
        printf("Number is not prime");
        }
    }
    return 0;
}

