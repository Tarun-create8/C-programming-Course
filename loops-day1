//While loop 
#include <stdio.h>
#include <stdlib.h>
int main(){
    int count=1;
    while(count<=10){
        printf("*");
        count=count+1;
    }
    return 0;
}

#include <stdio.h>
#include <stdlib.h>
int main(){
    int number, n, i;
    int result=1;
    printf("Enter number: ");
    scanf("%d", &number);
    printf("Enter Power: ");
    scanf("%d", &n);
    for(i=1;i<=n;++i){
        result= number*result;
    }
    printf("%d", result);
    return 0;
}

// Challange 
///challange 
#include <stdio.h>
#include <stdlib.h>
int main(){
    //to print no of star mentioned by user 
    int count=1, n;
    // printf("Enter Count: ");
    // scanf("%d", &count);
    printf("Enter n: ");
    scanf("%d", &n);
    while(count<=n){
        printf("*");
        count = count +1;
    }
    return 0;
}
